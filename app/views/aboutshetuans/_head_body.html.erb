<div class="head_body">
  <p class="main_title">
    关于社团
  </p>
  <div class="web_map">
    <%= link_to "首页", root_path %>
    <span class="map_go">>></span>
    <%= link_to "关于社团", aboutshetuans_path %>
      <% if admin_signed_in? %>
        (<span class="load"><%= link_to '添加社团', new_aboutshetuan_path %></span>)
      <% end %>
    <span class="map_go">>></span>
    <% if params[:controller] == 'aboutshetuans' and params[:action] == 'index' %>
      社团列表
      <span class="map_go">>></span>
      全部
    <% elsif params[:controller] == 'aboutshetuans' and params[:action] == 'show_kind' and params[:kind] == '社会公益类' %>
      社团列表
      <span class="map_go">>></span>
      社会公益类
    <% elsif params[:controller] == 'aboutshetuans' and params[:action] == 'show_kind' and params[:kind] == '体育运动类' %>
      社团列表
      <span class="map_go">>></span>
      体育运动类
    <% elsif params[:controller] == 'aboutshetuans' and params[:action] == 'show_kind' and params[:kind] == '文艺特长类' %>
      社团列表
      <span class="map_go">>></span>
      文艺特长类
    <% elsif params[:controller] == 'aboutshetuans' and params[:action] == 'show_kind' and params[:kind] == '学术创新类' %>
      社团列表
      <span class="map_go">>></span>
      学术创新类
    <% elsif params[:controller] == 'aboutshetuans' and params[:action] == 'show_kind' and params[:kind] == '应用实践类' %>
      社团列表
      <span class="map_go">>></span>
      应用实践类
    <% end %>
  </div>
</div>
